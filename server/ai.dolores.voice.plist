<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>ai.dolores.voice</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/node</string>
        <string>/Users/dolores/dolores-voice/server/index.js</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Users/dolores/dolores-voice/server</string>
    
    <key>UserName</key>
    <string>dolores</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/Users/dolores/dolores-voice/server/logs/voice-server.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/dolores/dolores-voice/server/logs/voice-server-error.log</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>
